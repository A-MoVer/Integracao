{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { auth } from \"@/firebase\";\nimport { GoogleAuthProvider, signInWithPopup } from \"firebase/auth\";\nimport axios from \"axios\";\nexport default {\n  name: \"LoginPage\",\n  methods: {\n    async loginWithGoogle() {\n      try {\n        const provider = new GoogleAuthProvider();\n        const result = await signInWithPopup(auth, provider);\n        const user = result.user;\n\n        // Obter o token do utilizador autenticado\n        const token = await user.getIdToken();\n        console.log(\"Token gerado durante o login:\", token);\n\n        // Guardar o token no localStorage para reutilização\n        localStorage.setItem(\"userToken\", token);\n\n        // Redirecionar após login (ou outra lógica necessária)\n        this.$router.push(\"/home\");\n      } catch (error) {\n        console.error(\"Erro ao fazer login:\", error);\n        alert(\"Não foi possível efetuar o login. Tente novamente.\");\n      }\n    }\n  }\n};","map":{"version":3,"names":["auth","GoogleAuthProvider","signInWithPopup","axios","name","methods","loginWithGoogle","provider","result","user","token","getIdToken","console","log","localStorage","setItem","$router","push","error","alert"],"sources":["C:\\Users\\Diogo Resende\\Desktop\\DWA\\G7_24-25\\G7_24-25\\Receitas\\frontend\\src\\components\\LoginPage.vue"],"sourcesContent":["<template>\r\n  <div class=\"login-container\">\r\n    <div class=\"login-card\">\r\n      <img src=\"@/assets/icon.png\" alt=\"Logo\" class=\"logo\" />\r\n      <h2>Entrar com serviços externos</h2>\r\n      <button class=\"login-btn google-login\" @click=\"loginWithGoogle\">\r\n        Entrar com Google\r\n      </button>\r\n      <p class=\"admin-link\">\r\n        É Administrador?\r\n        <a @click=\"$router.push('/admin-login')\" style=\"cursor: pointer;\">\r\n          Fazer login como Admin\r\n        </a>\r\n      </p>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { auth } from \"@/firebase\";\r\nimport { GoogleAuthProvider, signInWithPopup } from \"firebase/auth\";\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: \"LoginPage\",\r\n  methods: {\r\n    async loginWithGoogle() {\r\n  try {\r\n    const provider = new GoogleAuthProvider();\r\n    const result = await signInWithPopup(auth, provider);\r\n    const user = result.user;\r\n\r\n    // Obter o token do utilizador autenticado\r\n    const token = await user.getIdToken();\r\n    console.log(\"Token gerado durante o login:\", token);\r\n\r\n    // Guardar o token no localStorage para reutilização\r\n    localStorage.setItem(\"userToken\", token);\r\n\r\n    // Redirecionar após login (ou outra lógica necessária)\r\n    this.$router.push(\"/home\");\r\n  } catch (error) {\r\n    console.error(\"Erro ao fazer login:\", error);\r\n    alert(\"Não foi possível efetuar o login. Tente novamente.\");\r\n  }\r\n}\r\n\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.login-container {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  height: 100vh;\r\n  background-image: url(\"@/assets/background.jpg\");\r\n  background-size: cover;\r\n  background-repeat: no-repeat;\r\n  background-position: center;\r\n}\r\n\r\n.login-card {\r\n  background: white;\r\n  padding: 30px;\r\n  border-radius: 8px;\r\n  box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);\r\n  text-align: center;\r\n  width: 300px;\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n\r\n.logo {\r\n  width: 80px;\r\n  height: 80px;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.google-login {\r\n  background: #4285f4;\r\n  color: white;\r\n  border: none;\r\n  padding: 10px 20px;\r\n  font-size: 1em;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.google-login:hover {\r\n  background: #357ae8;\r\n}\r\n\r\n.admin-link {\r\n  font-size: 0.9em;\r\n}\r\n\r\n.admin-link a {\r\n  color: #007bff;\r\n  text-decoration: none;\r\n}\r\n\r\n.admin-link a:hover {\r\n  text-decoration: underline;\r\n}\r\n</style>\r\n"],"mappings":";AAmBA,SAASA,IAAG,QAAS,YAAY;AACjC,SAASC,kBAAkB,EAAEC,eAAc,QAAS,eAAe;AACnE,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,OAAO,EAAE;IACP,MAAMC,eAAeA,CAAA,EAAG;MAC1B,IAAI;QACF,MAAMC,QAAO,GAAI,IAAIN,kBAAkB,CAAC,CAAC;QACzC,MAAMO,MAAK,GAAI,MAAMN,eAAe,CAACF,IAAI,EAAEO,QAAQ,CAAC;QACpD,MAAME,IAAG,GAAID,MAAM,CAACC,IAAI;;QAExB;QACA,MAAMC,KAAI,GAAI,MAAMD,IAAI,CAACE,UAAU,CAAC,CAAC;QACrCC,OAAO,CAACC,GAAG,CAAC,+BAA+B,EAAEH,KAAK,CAAC;;QAEnD;QACAI,YAAY,CAACC,OAAO,CAAC,WAAW,EAAEL,KAAK,CAAC;;QAExC;QACA,IAAI,CAACM,OAAO,CAACC,IAAI,CAAC,OAAO,CAAC;MAC5B,EAAE,OAAOC,KAAK,EAAE;QACdN,OAAO,CAACM,KAAK,CAAC,sBAAsB,EAAEA,KAAK,CAAC;QAC5CC,KAAK,CAAC,oDAAoD,CAAC;MAC7D;IACF;EAEE;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}